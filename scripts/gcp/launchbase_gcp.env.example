# LaunchBase Google Cloud bootstrap config
# Copy to launchbase_gcp.env and edit values.

PROJECT_ID=engaged-style-456320-t4
PROJECT_NUMBER=458208722363
REGION=us-central1
MULTI_REGION=US

# Service Accounts
ORCHESTRATOR_SA_ID=launchbase-ai-orchestrator
PIPELINE_RUNNER_SA_ID=launchbase-vertex-runner

# GCS Buckets (must be globally unique)
MODELS_BUCKET=lb-ai-models-engaged-style-456320-t4-us
DATA_BUCKET=lb-ai-data-engaged-style-456320-t4-us
ARTIFACTS_BUCKET=lb-ai-artifacts-engaged-style-456320-t4-us
PIPELINE_BUCKET=lb-ai-pipelines-engaged-style-456320-t4-us

# BigQuery
BQ_DATASET_MARKETING=launchbase_marketing
BQ_DATASET_TRAINING=launchbase_training
BQ_DATASET_EVAL=launchbase_eval

# Artifact Registry
AR_REPO=launchbase-ai

# Secret names (values are created as placeholders; set real values after bootstrap)
SECRET_NAMES=aimlapi-key,openai-api-key,google-ads-dev-token,meta-ads-access-token,hubspot-api-key,resend-api-key

# Default container image to run in Vertex custom jobs
TRAINING_IMAGE_URI=us-central1-docker.pkg.dev/engaged-style-456320-t4/launchbase-ai/marketing-trainer:latest

# Model lane manifests (scientist handoff)
MODEL_MAIN_MANIFEST=gs://lb-ai-models-engaged-style-456320-t4-us/main-8b-governed/manifests/model.manifest.json
MODEL_SANDBOX_MANIFEST=gs://lb-ai-models-engaged-style-456320-t4-us/sandbox-8b-isolated/manifests/model.manifest.json

# Marketing sandbox bridge
MARKETING_ENABLE_CYCLE_BRIDGE=true
MARKETING_ENABLE_PI_SANDBOX=true
MARKETING_ENABLE_OBLITERATED_SANDBOX=true
MARKETING_ALLOW_SANDBOX_EXECUTE=false
MARKETING_PI_SANDBOX_URL=https://pi-agent-sandbox-6af67etolq-uc.a.run.app
MARKETING_PI_PRIMARY_MODEL=openai/gpt-5-2-codex
MARKETING_PI_REVIEW_MODEL=claude-sonnet-4-6
